{
  "name": "tourze/jiguang-push-bundle",
  "description": "极光推送",
  "type": "symfony-bundle",
  "require": {
    "php": "^8.1",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^2.17 || ^3.0",
    "symfony/framework-bundle": "^6.4",
    "tourze/arrayable": "0.0.*",
    "tourze/easy-admin-attribute": "0.0.*",
    "tourze/doctrine-indexed-bundle": "0.0.*",
    "tourze/doctrine-timestamp-bundle": "0.0.*",
    "tourze/doctrine-user-bundle": "0.0.*",
    "tourze/http-client-bundle": "0.0.*",
    "tourze/doctrine-track-bundle": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "JiguangPushBundle\\": "src"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  }
}